{
  "name": "publisher",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run build:styles",
    "prest": "cross-env NODE_ENV=production npm run build:prod:styles",
    "start": "cross-env NODE_ENV=development node ./bin/www",
    "startonly": "cross-env NODE_ENV=development node ./bin/www",
    "start:production": "cross-env NODE_ENV=production node ./bin/www",
    "st:docker": "cross-env NODE_ENV=production npm run build:prod:styles && cross-env NODE_ENV=production node ./bin/www",
    "build:styles": "npx tailwindcss -i styles.css -o public/stylesheets/styles_generated.css",
    "build:prod:styles": "npx tailwindcss -i styles.css -o public/stylesheets/styles_generated.css",
    "build:watch": "npx tailwindcss -i styles.css -o public/stylesheets/styles_generated.css --watch"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.2.4",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-handlebars": "^5.2.0",
    "express-minify-html": "^0.12.0",
    "googleapis": "^84.0.0",
    "handlebars-extend-block": "^1.0.0",
    "handlebars-helpers": "^0.10.0",
    "handlebars-layout": "^0.1.4",
    "hbs": "~4.0.4",
    "lodash": "^4.17.20",
    "marked": "^2.1.3",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^4.2.0",
    "tailwindcss": "^3.2.7",
    "xml": "^1.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
